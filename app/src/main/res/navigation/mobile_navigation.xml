<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.restaurantreview.ui.home.HomeFragment"
        android:label="@string/main_tab"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.restaurantreview.ui.UpComing.UpComingFragment"
        android:label="@string/tab_1"
        tools:layout="@layout/fragment_upcoming" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.restaurantreview.ui.Finished.FinishedFragment"
        android:label="@string/tab_2"
        tools:layout="@layout/fragment_finished" />
</navigation>